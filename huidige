  content: "";
  display: table;
  clear: both;
}

/* Style the footer */
footer {
  background-color: #0a0a0a;
  padding: 10px;
  text-align: center;
  color: white;
}

/* Responsive layout - makes the two columns/boxes stack on top of each other instead of next to each other, on small screens */
@media (max-width: 600px) {
  nav, article {
    width: 100%;
    height: auto;
  }
}
